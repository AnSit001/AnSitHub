plr = game:GetService("Players").LocalPlayer
local af,mob = false,""
loadstring(game:HttpGetAsync("https://pastebin.com/raw/5jmSeb7W"))()
local gui = library:AddWindow("An Sit Dz - AutoFARM", {
    main_color = Color3.fromRGB(37, 96, 161),
    min_size = Vector2.new(380, 325),
    can_resize = true
})

game:GetService("StarterGui"):SetCore("SendNotification", {
    Title = "An Sit Dz";
            Text = "La Mot Nguoi Rat Dep Trai"})
            
local main = gui:AddTab("Main Tab")
main:AddSwitch("Auto-Farm",function(bool)
    af = bool
end)
local main2 = gui:AddTab("Shutdown")
main2:AddSwitch("Server Shutdown",function(bool)
    game.StarterGui:SetCore("SendNotification", {
        Title = "Info server!";
        Text = "The server will shut down in less than 1 minute.";
        Icon = "";
        Duration = 60;
        Callback = bindableFunction;
        Button1 = "Yes"; 
    })
    game:GetService("Workspace").Script.Hours.Value = 0
    game:GetService("Workspace").Script.Minutes.Value = 0
    game:GetService("Workspace").Script.Seconds.Value = 0
end)
mobsDrop = main:AddDropdown("Mobs",function(a)
    mob = a
end)
local mobs = {}
spawn(function()
    while wait() do
        for i,v in pairs(workspace:GetChildren()) do
            if v:FindFirstChild("HumanoidRootPart") and v:FindFirstChild("Humanoid") and not game.Players:FindFirstChild(v.Name) and not table.find(mobs,v.Name) then
                table.insert(mobs,v.Name)
                mobsDrop:Add(v.Name)
            end
        end
    end
end)
main:AddButton("GUI KILL",function()
    for i,v in pairs(game:GetService("CoreGui"):GetChildren()) do
        if v:FindFirstChild("Prefabs") then
            v:Destroy()
        end
    end
end)
function farmMobs()
    pcall(function()
        for i,v in pairs(workspace:GetChildren()) do
            if v.Name == mob and v:FindFirstChild("HumanoidRootPart") and v:FindFirstChild("Humanoid") and v.Humanoid.Health > 0 then
                repeat wait()
                    if af then
                        plr.Character.HumanoidRootPart.CFrame = v.HumanoidRootPart.CFrame - v.HumanoidRootPart.CFrame.lookVector * 4
                        spawn(function()
                            game:GetService("ReplicatedStorage").Remotes.Damage4:InvokeServer(v,{
                                ["HitTime"] = 0,
                                ["Type"] = "Normal",
                                ["HitEffect"] = game.ReplicatedStorage.Resources.OtherEffects.NormalHitEffect2,
                                ["VictimCFrame"] = v.HumanoidRootPart.CFrame,
                                ["HurtAnimation"] = game.ReplicatedStorage.Resources.Animations.HurtAnimations.Hurt1,
                                ["Velocity"] = Vector3.new(),
                                ["Sound"] = game.ReplicatedStorage.Resources.Sounds.Combat.NormalPunch,
                                ["Damage"] = 1
                            })
                        end)
                    end
                until not af or v.Humanoid.Health <= 0 or not v:IsDescendantOf(workspace)
            end
        end
    end)
end
spawn(function()
    while wait() do
        if af then
            farmMobs()
        end
    end
end)
game.RunService.Heartbeat:Connect(function()
    if af then
        pcall(function()
            plr.Character.Humanoid:ChangeState(10)
        end)
    end
end)
